public class Alumno {
		//atributos
		private String nroLibretaUniversitaria;

		public String getNroLibretaUniversitaria() {
			return nroLibretaUniversitaria;
		}

		public void setNroLibretaUniversitaria(String nroLibretaUniversitaria) {
			this.nroLibretaUniversitaria = nroLibretaUniversitaria;
		}
		
}

